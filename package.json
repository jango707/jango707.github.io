{
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "mongoose": "^5.9.16",
    "react-router-dom": "^5.2.0"
  },
  "name": "personalwebsite",
  "version": "1.0.0",
  "description": "website Server",
  "main": "index.js",
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "client-install": "npm install --prefix personalwebsite",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix personalwebsite",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix personalwebsite && npm run build --prefix personalwebsite",
    "predeploy": "npm run build --prefix personalwebsite",
    "deploy": "gh-pages -d personalwebsite/build"
  },
  "homepage": "http://jangosblog.me/",
  "author": "JANGO",
  "license": "ISC"
}
